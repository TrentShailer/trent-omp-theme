{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"blocks": [
		{
			"alignment": "left",
			"newline": true,
			"type": "prompt",
			"segments": [
				{
					"type": "session",
					"style": "plain",
					"foreground": "#E95678",
					"template": "{{ .UserName }} <#FEF5ED>at</>"
					
				},
				{
					"type": "time",
					"style": "plain",
					"foreground": "#F09383",
					"properties": {
						"time_format": "3:04pm Monday"
					},
					"template": " {{ .CurrentDate | date .Format }} "
				},
				{
					"type": "git",
					"style": "plain",
					"foreground": "#FAB795",
					"properties": {
						"branch_icon": "\ue725 "
					},
					"template": "<#FEF5ED>on</> {{ .HEAD }} "
				}
			]
		},
		{
			"alignment": "left",
			"newline": true,
			"type": "prompt",
			"segments": [
				{
					"type": "path",
					"style": "plain",
					"foreground": "#FAC29A",
					"properties": {
						"folder_separator_icon": "<b>/</b>",
						"home_icon": "~",
						"style": "agnoster_full"
					},
					"template": "\ue5ff {{ .Path }} "
				}
			]
		},
		{
			"alignment": "left",
			"newline": true,
			"segments": [
				{
					"foreground": "#FAB795",
					"leading_diamond": "<#F09383>\uf054</> ",
					"style": "diamond",
					"template": "</>",
					"type": "path"
				}
			],
			"type": "prompt"
		}
	],
	"console_title_template": "{{ .Folder }}",
	"transient_prompt": {
		"background": "transparent",
		"foreground": "#FEF5ED"
	},
	"version": 2
}
