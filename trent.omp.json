{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"palette": {
		"text_fg": "#aaaaaa",
		"text_bg": "#1d1d1d",
		"background": "#101010"
	},
	"terminal_background": "p:background",
	"accent_color": "p:text_fg",
	"blocks": [
		{
			"alignment": "left",
			"newline": true,
			"type": "prompt",
			"segments": [
				{
					"type": "session",
					"style": "plain",
					"foreground": "p:text_fg",
					"template": "{{ .UserName }}"
				},
				{
					"type": "text",
					"style": "plain",
					"foreground": "p:text_bg",
					"template": " in "
				},
				{
					"type": "path",
					"style": "plain",
					"foreground": "p:text_fg",
					"properties": {
						"folder_icon": "..",
						"folder_separator_icon": "/",
						"home_icon": "~",
						"style": "agnoster_short",
						"max_depth": 3
					},
					"template": "{{ .Path }}"
				},
				{
					"type": "git",
					"style": "plain",
					"foreground": "p:text_bg",
					"template": " on "
				},
				{
					"type": "git",
					"style": "plain",
					"foreground": "p:text_fg",
					"properties": {
						"branch_icon": "\ue725 "
					},
					"template": "{{ .HEAD }} "
				}
			]
		},
		{
			"alignment": "left",
			"newline": true,
			"segments": [],
			"type": "prompt"
		},
		{
			"alignment": "left",
			"newline": true,
			"segments": [
				{
					"style": "plain",
					"foreground": "p:text_fg",
					"template": "\uf105",
					"type": "text"
				}
			],
			"type": "prompt"
		}
	],
	"final_space": true,
	"version": 2
}
